<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/blue"
    tools:context=".MainActivity"
    tools:layout_editor_absoluteY="81dp">

    <TextView
        android:id="@+id/title"
        android:layout_width="0dp"
        android:layout_height="98dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginBottom="84dp"
        android:layout_marginEnd="17dp"
        android:layout_marginStart="17dp"
        android:layout_marginTop="29dp"
        android:background="@android:color/transparent"
        android:fontFamily="serif"
        android:gravity="center_vertical"
        android:text="CS: Mentor-Mentee"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textColor="@color/colorDarkPurple"
        android:textSize="36sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/imageVie4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="NotSibling" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="199dp"
        android:layout_height="75dp"
        android:layout_marginBottom="1dp"
        android:layout_marginEnd="13dp"
        android:fontFamily="cursive"
        android:text="Academic Session 2018/2019"
        android:textAlignment="center"
        android:textColor="@color/colorPrimaryDark"
        android:textSize="30sp"
        android:textStyle="italic"
        app:layout_constraintBottom_toTopOf="@+id/relativeLayout4"
        app:layout_constraintEnd_toEndOf="@+id/title"
        app:layout_constraintTop_toBottomOf="@+id/title"
        android:layout_marginRight="13dp" />

    <RelativeLayout
        android:id="@+id/relativeLayout4"
        android:layout_width="400dp"
        android:layout_height="253dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView6">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="35dp"
            android:layout_height="40dp"
            android:layout_alignLeft="@+id/imageView4"
            android:layout_alignStart="@+id/imageView4"
            android:layout_alignTop="@+id/mentorName"
            android:background="@drawable/roundwhite"
            app:srcCompat="@drawable/mentee"
            tools:ignore="NotSibling" />

        <ImageView
            android:id="@+id/imageVi"
            android:layout_width="35dp"
            android:layout_height="40dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:background="@drawable/roundwhite"
            app:srcCompat="@drawable/mentee" />

        <ListView
            android:id="@+id/listmentee"
            android:layout_width="152dp"
            android:layout_height="200dp"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_below="@+id/imageView4"
            tools:layout_editor_absoluteX="200dp"
            tools:layout_editor_absoluteY="236dp" />


        <TextView
            android:id="@+id/menteeV"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/imageVi"
            android:layout_toRightOf="@+id/imageVi"
            android:background="@android:color/transparent"
            android:fontFamily="sans-serif"
            android:gravity="center_horizontal"
            android:text="Mentee"
            android:textAlignment="viewStart"
            android:textColor="@android:color/black"
            android:textSize="22sp"
            android:textStyle="bold"
            tools:ignore="RtlCompat" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="45dp"
            android:layout_height="40dp"
            android:layout_alignTop="@+id/calendarView"
            android:layout_toEndOf="@+id/listmentee"
            android:layout_toRightOf="@+id/listmentee"
            android:background="@android:color/transparent"
            app:srcCompat="@drawable/calendar"
            tools:layout_editor_absoluteX="373dp"
            tools:layout_editor_absoluteY="0dp" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="45dp"
            android:layout_height="40dp"
            android:layout_below="@+id/imageView2"
            android:layout_marginTop="16dp"
            android:layout_toLeftOf="@+id/AnnView"
            android:layout_toStartOf="@+id/AnnView"
            android:background="@android:color/transparent"
            app:srcCompat="@drawable/anouncement" />


        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="45dp"
            android:layout_height="40dp"
            android:layout_below="@+id/imageView3"
            android:layout_marginTop="16dp"
            android:layout_toEndOf="@+id/listmentee"
            android:layout_toRightOf="@+id/listmentee"
            android:background="@android:color/transparent"
            app:srcCompat="@drawable/mentees" />

        <TextView
            android:id="@+id/calendarView"
            android:layout_width="158dp"
            android:layout_height="40dp"
            android:layout_alignTop="@+id/listmentee"
            android:layout_marginTop="25dp"
            android:layout_toEndOf="@+id/imageView2"
            android:layout_toRightOf="@+id/imageView2"
            android:autoLink="none"
            android:background="@drawable/rectangleborder"
            android:clickable="true"
            android:text=" Calendar"
            android:textColor="@android:color/black"
            android:textSize="22sp"
            android:textStyle="bold"
            tools:layout_editor_absoluteX="233dp" />

        <TextView
            android:id="@+id/AnnView"
            android:layout_width="158dp"
            android:layout_height="40dp"
            android:layout_alignTop="@+id/imageView3"
            android:layout_toEndOf="@+id/imageView2"
            android:layout_toRightOf="@+id/imageView2"
            android:background="@drawable/rectangleborder"
            android:clickable="true"
            android:gravity="center_horizontal"
            android:text=" Announcement"
            android:textAlignment="viewStart"
            android:textColor="@android:color/black"
            android:textSize="22sp"
            android:textStyle="bold"
            tools:ignore="RtlCompat"
            tools:layout_editor_absoluteX="233dp" />

        <TextView
            android:id="@+id/acaV"
            android:layout_width="158dp"
            android:layout_height="40dp"
            android:layout_alignTop="@+id/imageView5"
            android:layout_toEndOf="@+id/imageView5"
            android:layout_toRightOf="@+id/imageView5"
            android:background="@drawable/rectangleborder"
            android:fontFamily="sans-serif"
            android:gravity="start"
            android:text=" Academic"
            android:textAlignment="viewStart"
            android:textColor="@android:color/black"
            android:textSize="22sp"
            android:textStyle="bold" />


    </RelativeLayout>

</android.support.constraint.ConstraintLayout>